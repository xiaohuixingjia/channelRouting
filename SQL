CREATE TABLE
    DS_CHANNEL_DATA_SOURCE
    (
        ID VARCHAR(8) NOT NULL,
        SUPPLIER_ID VARCHAR(20) NOT NULL,
        NAME VARCHAR(200) DEFAULT '通道路由数据源' NOT NULL,
        URL VARCHAR(200) DEFAULT '通道路由数据源请求路径' NOT NULL,
        STATE SMALLINT DEFAULT 2 NOT NULL,
        INTIME TIMESTAMP DEFAULT CURRENT TIMESTAMP NOT NULL,
        HBASE_STATE INTEGER DEFAULT 2,
        HBASE_TABLE VARCHAR(50) DEFAULT '',
        HBASE_RESULT VARCHAR(50) DEFAULT '',
        KEY_NAMES VARCHAR(300) DEFAULT '',
        ENCRYPT_TIME INTEGER DEFAULT 2,
        CACHE_STATE INTEGER DEFAULT 4,
        NO_CACHE_RESU VARCHAR(300) DEFAULT '',
        PRIMARY KEY (ID)
    );
COMMENT ON TABLE DS_CHANNEL_DATA_SOURCE
IS
    '数据源表';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.ID
IS
    '数据源ID';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.SUPPLIER_ID
IS
    '供应商ID';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.NAME
IS
    '数据源名称';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.URL
IS
    '数据源请求路径';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.STATE
IS
    '数据源状态（是否可用 2可用  4不可用）';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.INTIME
IS
    '记录创建时间:格式YYYY-MM-DD HH24:MI:SS';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.HBASE_TABLE
IS
    'hbase表名';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.HBASE_RESULT
IS
    'hbase查得的返回结果';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.KEY_NAMES
IS
    '从hbase取值的key标签，用,拼接';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.ENCRYPT_TIME
IS
    'md5加密次数';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.CACHE_STATE
IS
    '是否使用缓存';
COMMENT ON COLUMN DS_CHANNEL_DATA_SOURCE.NO_CACHE_RESU
IS
    '不加入缓存的数据集合';
    
    
    
CREATE TABLE
    DS_CHANNEL_ROUTING
    (
        PRODUCT_SUITES_CODE VARCHAR(50) NOT NULL,
        MER_CODE VARCHAR(20) NOT NULL,
        OPERATOR_TYPE SMALLINT NOT NULL,
        DS_ID VARCHAR(8) NOT NULL,
        SORT INTEGER NOT NULL,
        INTIME TIMESTAMP DEFAULT CURRENT TIMESTAMP NOT NULL,
        WEIGHT INTEGER DEFAULT 1,
        USE_NEXT INTEGER DEFAULT 4,
        HBASE_STATE INTEGER,
        CACHE_STATE INTEGER,
        PRIMARY KEY (PRODUCT_SUITES_CODE, MER_CODE, OPERATOR_TYPE, DS_ID)
    );
COMMENT ON TABLE DS_CHANNEL_ROUTING
IS
    '通道路由信息表';
COMMENT ON COLUMN DS_CHANNEL_ROUTING.PRODUCT_SUITES_CODE
IS
    '产品包编码';
COMMENT ON COLUMN DS_CHANNEL_ROUTING.MER_CODE
IS
    '商户编码';
COMMENT ON COLUMN DS_CHANNEL_ROUTING.OPERATOR_TYPE
IS
    '运营商类型（0（未识别）1（移动）2（联通）3（电信））';
COMMENT ON COLUMN DS_CHANNEL_ROUTING.DS_ID
IS
    '数据源ID';
COMMENT ON COLUMN DS_CHANNEL_ROUTING.SORT
IS
    '排名';
COMMENT ON COLUMN DS_CHANNEL_ROUTING.INTIME
IS
    '记录创建时间:格式YYYY-MM-DD HH24:MI:SS';
COMMENT ON COLUMN DS_CHANNEL_ROUTING.WEIGHT
IS
    '权重';
COMMENT ON COLUMN DS_CHANNEL_ROUTING.USE_NEXT
IS
    '未查得是否使用下一个通道 2使用 4不使用';
    
    
    
CREATE TABLE
    DS_CHANNEL_SUPPLIER
    (
        ID VARCHAR(8) NOT NULL,
        NAME VARCHAR(200) DEFAULT '数据源供应商' NOT NULL,
        INTIME TIMESTAMP DEFAULT CURRENT TIMESTAMP NOT NULL,
        PRIMARY KEY (ID)
    );
COMMENT ON TABLE DS_CHANNEL_SUPPLIER
IS
    '数据源供应商表';
COMMENT ON COLUMN DS_CHANNEL_SUPPLIER.ID
IS
    '供应商ID';
COMMENT ON COLUMN DS_CHANNEL_SUPPLIER.NAME
IS
    '供应商名称';
COMMENT ON COLUMN DS_CHANNEL_SUPPLIER.INTIME
IS
    '记录创建时间:格式YYYY-MM-DD HH24:MI:SS';